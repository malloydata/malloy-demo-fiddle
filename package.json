{
  "name": "malloy-duckdb-wasm",
  "version": "0.0.2",
  "description": "",
  "main": "./dist/index.js",
  "config": {
    "malloy_packages": [
      "@malloydata/db-duckdb",
      "@malloydata/malloy",
      "@malloydata/render"
    ]
  },
  "scripts": {
    "start": "NODE_ENV=development npm run build 8888",
    "watch": "NODE_ENV=development npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "ts-node scripts/build",
    "lint": "eslint .",
    "malloy-install": "npm --no-fund --no-audit install --save-exact $npm_package_config_malloy_packages",
    "malloy-update": "npm --no-fund --no-audit unlink $npm_package_config_malloy_packages && npm run malloy-install",
    "malloy-link": "npm --no-fund --no-audit link $npm_package_config_malloy_packages",
    "malloy-unlink": "npm --no-fund --no-save --no-audit unlink $npm_package_config_malloy_packages && npm --no-fund --no-audit install --force"
  },
  "author": "Malloy",
  "license": "GPL-2.0",
  "dependencies": {
    "@malloydata/db-duckdb": "^0.0.6",
    "@malloydata/malloy": "^0.0.6",
    "@malloydata/render": "^0.0.6",
    "apache-arrow": "^9.0.0",
    "monaco-editor": "^0.34.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.18.0",
    "esbuild": "0.14.13",
    "esbuild-plugin-svgr": "^1.0.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "ts-node": "^10.9.1"
  }
}
